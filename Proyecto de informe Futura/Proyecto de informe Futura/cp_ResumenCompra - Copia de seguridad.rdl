  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="IdProveedor">
          <DataField>IdProveedor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Proveedor">
          <DataField>Proveedor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Exenta">
          <DataField>Exenta</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Gravada">
          <DataField>Gravada</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Iva">
          <DataField>Iva</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT 
 C.Proveedor IdProveedor
, p.Razon Proveedor
,SUM(c.exenta) Exenta
,SUM(C.gravada) Gravada
,SUM(iva) Iva
FROM cp_orden_compra c
INNER JOIN cp_proveedor p ON p.IdProveedor=c.Proveedor
where Est_Ord='F'
AND (c.Fec_Envio &gt;= @FechaDesde or @FechaDesde is null) and (c.Fec_Envio&lt;=@FechaHasta OR @Fecha_Hasta is null)
GROUP BY C.Proveedor,p.Razon</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@FechaDesde">
            <Value>=Parameters!FechaDesde.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FechaHasta">
            <Value>=Parameters!FechaHasta.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Fecha_Hasta">
            <Value>=Parameters!Fecha_Hasta.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
