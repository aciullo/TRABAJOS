  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="FechaPago">
          <DataField>FechaPago</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FechaCompra">
          <DataField>FechaCompra</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="nrorecibo">
          <DataField>nrorecibo</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="facturaproveedor">
          <DataField>facturaproveedor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="proveedor">
          <DataField>proveedor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="importe_pag">
          <DataField>importe_pag</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="total">
          <DataField>total</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="idmoneda">
          <DataField>idmoneda</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     a.fecha FechaPago,f.Fecha FechaCompra, a.nrocomprobante AS nrorecibo, f.facturaproveedor, RTRIM(a.idproveedor) + '-' + c.Razon AS proveedor, 
		SUM(b.importe_pag) AS importe_pag,sum(ISNULL(F.exenta,0)+ISNULL(F.gravada,0)+ISNULL(F.iva,0)) total, a.idmoneda
FROM  cp_pagos_base AS a 
INNER JOIN   cp_pagosdet_base AS b ON a.idpago = b.idpago 
INNER JOIN  cp_factura AS f ON f.IdEmpresa=b.idempresa 
AND b.facturaproveedor=f.FacturaProveedor AND F.IdProveedor=B.idproveedor AND F.IdMoneda=B.idmoneda
LEFT OUTER JOIN   cp_proveedor AS c ON a.idproveedor = c.IdProveedor AND a.idempresa = c.IdEmpresa
WHERE 
a.idempresa=@idempresa and (a.fecha BETWEEN @dfecha and @hfecha ) 
and (a.sucursal=@sucursal or @sucursal is null) 
and (a.idproveedor = @idproveedor or @idproveedor is null)
GROUP BY a.fecha,f.Fecha , a.nrocomprobante, RTRIM(a.idproveedor) + '-' + c.Razon, a.idmoneda,f.facturaproveedor
ORDER BY a.idmoneda, a.fecha, proveedor</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@idempresa">
            <Value>=Parameters!idempresa.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dfecha">
            <Value>=Parameters!dfecha.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@hfecha">
            <Value>=Parameters!hfecha.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@sucursal">
            <Value>=Parameters!sucursal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@idproveedor">
            <Value>=Parameters!idproveedor.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
