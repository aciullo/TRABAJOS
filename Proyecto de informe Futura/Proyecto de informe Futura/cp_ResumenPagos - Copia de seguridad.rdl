  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="fecha">
          <DataField>fecha</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="nrorecibo">
          <DataField>nrorecibo</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="proveedor">
          <DataField>proveedor</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="importe_pag">
          <DataField>importe_pag</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="idmoneda">
          <DataField>idmoneda</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="medio_Pago">
          <DataField>medio_Pago</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT  
  a.fecha
  , a.nrocomprobante AS nrorecibo
  , RTRIM(a.idproveedor) + '-' + c.Razon AS proveedor
  , SUM(b.importe_pag) AS importe_pag
  , a.idmoneda
  , ISNULL(MP.tip_valor,'EF') medio_Pago
FROM             cp_pagos_base	  AS a 
INNER JOIN       cp_pagosdet_base AS b ON a.idpago = b.idpago 
LEFT OUTER JOIN	 cp_detmed_pago   AS mp ON mp.idpago=b.idpago AND mp.idempresa=b.idempresa AND mp.idmoneda=b.idmoneda
LEFT OUTER JOIN  cp_proveedor     AS c ON a.idproveedor = c.IdProveedor AND a.idempresa = c.IdEmpresa
WHERE  a.idempresa=@idempresa 
and (a.fecha BETWEEN @dfecha and @hfecha ) 
	and (a.sucursal=@sucursal or @sucursal is null) 
	and (a.idproveedor = @idproveedor or @idproveedor is null)
GROUP BY a.fecha, a.nrocomprobante, RTRIM(a.idproveedor) + '-' + c.Razon, a.idmoneda,MP.tip_valor
ORDER BY a.idmoneda, a.fecha, proveedor</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@idempresa">
            <Value>=Parameters!idempresa.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@dfecha">
            <Value>=Parameters!dfecha.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@hfecha">
            <Value>=Parameters!hfecha.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@sucursal">
            <Value>=Parameters!sucursal.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@idproveedor">
            <Value>=Parameters!idproveedor.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
