      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select IdProveedor
, ISNULL(RAZON,'')Razon
,ISNULL(Contacto,'')Contacto 
,ISNULL(CargoContacto,'')CargoContacto
,ISNULL(pr.Ruc,'')RUC
,ISNULL(Direccion,'') Direccion 
,ISNULL(b.Descripcion,'') Barrio
,ISNULL(c.Descripcion,'') Ciudad
,ISNULL(PR.Telefono,'') Telefono
,ISNULL(pr.Fax,'') Fax
,ISNULL(p.Descripcion,'') Pais
,ISNULL(pr.Email,'') Email
,ISNULL(pr.CuentaContable,'') CuentaContable
,ISNULL(pr.Notas,'') Notas
,CASE pr.ACTIVO WHEN  1 THEN 'Activo' ELSE 'Inactivo' END Estado
,e.empresa Empresa
FROM cp_proveedor pr
INNER JOIN empresa e ON e.idempresa=pr.idempresa
left JOIN cp_Cartera car ON car.IdCartera=pr.idcartera and pr.IdEmpresa=car.IdEmpresa
left JOIN bs_Barrios b	  ON b.IdBarrio=pr.IdBarrio
left JOIN bs_Ciudades C   ON c.IdCiudad=pr.IdCiudad
left JOIN bs_Paises p	  ON p.IdPais=pr.Pais
WHERE pr.activo=1
and pr.IdEmpresa=@empresa
ORDER BY pr.Razon</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@empresa">
            <Value>=Parameters!empresa.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
