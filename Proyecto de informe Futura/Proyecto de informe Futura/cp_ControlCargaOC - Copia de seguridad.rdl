      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT     o.Fec_Ord AS Fecha
, o.Exenta
, o.Gravada
, o.Iva
, prov.IdProveedor + prov.Razon AS Proveedor
, case when ISNULL(p.Catalogo,'')='' then d.IdProducto else p.Catalogo end as IdProducto
, d.Cantidad
, d.Precio
, d.Descripcion as Producto
, d.Descripcion1
,referencia
, o.Contacto
,DireccionEnvio
,CiudadEnvio
,TelefonoEnvio
,o.Numero
, prov.Direccion
, prov.Telefono
, prov.Fax
, RTRIM(u.First_Name) + ' ' + u.Last_Name as Usuario
,o.IDMoneda
,o.Condicion
,o.Est_Ord
,e.Descripcion 
FROM         dbo.cp_orden_compra o 
	INNER JOIN   dbo.cp_det_ord d ON o.IdOrden = d.IdOrden 
	INNER JOIN   dbo.cp_proveedor prov ON o.IdEmpresa = prov.IdEmpresa AND o.Proveedor = prov.IdProveedor
	INNER JOIN ST_PRODUCTO p on d.IdEmpresa = p.IdEmpresa and d.IdProducto = p.IdProducto
	INNER JOIN bs_estados e ON  e.Cod_Esta=o.Est_Ord
	LEFT JOIN usuarios u on o.Audit_Usuario = u.Employee_Id collate Modern_Spanish_CI_AS
	where 
	o.idempresa=@idempresa
and ( (o.Fec_Ord between @dfecha and @hfecha) or  @dfecha IS NULL OR  @hfecha IS NULL )
AND (prov.idproveedor=@idproveedor or @idproveedor is null)
AND ((o.numero between  @dNroOrden and @hNroOrden ) OR  @dNroOrden IS NULL OR  @hNroOrden IS NULL)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
